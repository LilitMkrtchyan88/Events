<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.lilit.events">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Pages.UserTotalPage" />
        <activity android:name=".Pages.LoginPage" />
        <activity android:name=".Pages.UpdateForgotPasswordPage" />
        <activity android:name=".Pages.RegistrationUserPage" />
        <activity android:name=".Pages.HelpPage" />
        <activity
            android:name=".Pages.EventsPages.EventsListPage"
            android:label="@string/title_activity_events_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Pages.NotificationsPage" />
        <activity android:name=".Pages.MessagesPage" />
        <activity android:name=".Pages.EventsPages.EventPage" />
        <activity android:name=".Pages.MyPage" />
        <activity android:name=".Pages.EventsPages.NewEventPage" />
        <activity android:name=".Pages.CalendarPage" />
        <activity android:name=".Pages.ContactsPage" />
        <activity android:name=".Pages.EventsPages.MyEventsPage"></activity>
    </application>

</manifest>